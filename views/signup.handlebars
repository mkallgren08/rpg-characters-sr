
{{!-- method="post" action="/signup" --}}
{{!-- <script type = "text/javascript" src= "/components/Signup/signup.js"></script> --}}
<script type = "text/javascript" src= "{{pagelogic}}"></script>
<link rel="stylesheet" href="{{pagestyle}}">

<form id="signup" name="signup" method="post" action="signup"> 
  <label for="email">Email Address</label>
  <input class="text" name="email" type="email" />
  <br>
  <label for="firstname">Firstname</label>
  <input name="firstname" type="text" />
  <br>
  <label for="lastname">Lastname</label>
  <input name="lastname" type="text" />
  <br>
  <label for="password">Password</label>
  <input name="password" id = "pswd-set" type="password" />
  <br>
  <label for="password-confirm"> Confirm Password</label>
  <input name="password-confirm" id = "pswd-confirm" type="password" />
  <br>
  <input class="btn" type="submit" value="Sign Up" id = "signup-submit" />
</form>


